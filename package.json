{
  "name": "test_parcel_layout",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.3",
    "sass": "^1.16.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "scripts": {
    "dev": "parcel src/pages/*.html",
    "build": "parcel build src/pages/*.html",
    "format": "prettier --write src/**/*.{js,ts,json,scss,md}",
